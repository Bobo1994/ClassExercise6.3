
import java.awt.Dimension;
import java.awt.FlowLayout;

import javax.swing.JButton;
import javax.swing.JFrame;

public class SwingFlowLayouts extends JFrame {
	private JButton btn,btn1,btn2,btn3,btn4;
	public SwingFlowLayouts() {
	
		btn=new JButton("1");
		btn1=new JButton("2");
		btn2=new JButton("3");
		btn3=new JButton("4");
		btn4=new JButton("5");	
		setLayout(new FlowLayout());
		add(btn);
		add(btn1);
		add(btn2);
		add(btn3);
		add(btn4);
		setVisible(true);
		setSize(new Dimension(500,500));
}
}
